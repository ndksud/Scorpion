<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">450</defaultcontrol>
	<controls>
		<control type="group">
			<include>DialogAnimation</include>
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>1260</width>
			<height>728</height>
			<control type="image">
				<left>20</left>
				<top>78</top>
				<width>1220</width>
				<height>620</height>
				<texture border="40">50d.png</texture>
			</control>
			<control type="image">
				<width>100%</width>
				<height>100%</height>
				<texture border="40">85.png</texture>
			</control>
			<control type="label" id="1">
				<left>30</left>
				<top>32</top>
				<width>1068</width>
				<height>40</height>
				<font>font50</font>
				<label>$LOCALIZE[1036]</label>
				<align>center</align>
				<textcolor>white2</textcolor>
			</control>
			<control type="panel" id="450">
				<left>30</left>
				<top>88</top>
				<width>1200</width>
				<height>600</height>
				<onup>450</onup>
				<ondown>450</ondown>
				<onleft>60</onleft>
				<onright>60</onright>
				<pagecontrol>60</pagecontrol>
				<scrolltime tween="quadratic" easing="out">200</scrolltime>
				<itemlayout height="300" width="200">
					<control type="image">
						<width>200</width>
						<height>300</height>
						<texture>$INFO[ListItem.Icon]</texture>
						<bordersize>5</bordersize>
					</control>
				</itemlayout>
				<focusedlayout height="300" width="200">
					<control type="image">
						<width>200</width>
						<height>300</height>
						<texture>$INFO[ListItem.Icon]</texture>
						<bordersize>5</bordersize>
						<animation effect="zoom" start="100" end="120" center="100,150" time="200">Focus</animation>
						<animation effect="zoom" start="120" end="100" center="100,150" time="300">UnFocus</animation>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>662</left>
				<top>88</top>
				<height>420</height>
				<width>14</width>
				<onleft>450</onleft>
				<onright>450</onright>
				<ondown>60</ondown>
				<onup>60</onup>
				<showonepage>false</showonepage>
				<texturesliderbackground></texturesliderbackground>
				<texturesliderbar colordiffuse="55ffffff" border="4">scrollbar/texturesliderbar3.png</texturesliderbar>
				<texturesliderbarfocus border="4">scrollbar/texturesliderbar3.png</texturesliderbarfocus>
			</control>
		</control>
	</controls>
</window>